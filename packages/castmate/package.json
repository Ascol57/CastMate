{
  "name": "castmate",
  "version": "0.4.0",
  "description": "CastMate is a broadcaster tool that allows Twitch viewers to interact with a broadcasters stream components through Chat Commands, Channel Point rewards, and more.",
  "author": "LordTocs",
  "scripts": {
    "build": "vite build --mode production && yarn electron:build",
    "build:test": "vite build --mode production",
    "dev": "vite",
    "electron:build": "electron-builder --config electron-builder-config.cjs",
    "build:publish": "pnpm run build --publish always",
    "build:run": "pnpm run build & cd release/win-unpacked/ && CastMate.exe"
  },
  "main": "dist/electron/main/backgroundLoader.js",
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "electronVersion": "20.0.1",
  "dependencies": {
    "@peter-murray/hue-bridge-model": "^2.0.4",
    "@twurple/api": "^5.0.13",
    "@twurple/auth": "^5.0.13",
    "@twurple/chat": "^5.0.13",
    "@twurple/eventsub-ws": "^5.3.3",
    "@twurple/pubsub": "^5.0.13",
    "async-mutex": "^0.3.0",
    "axios": "^0.27.2",
    "bad-words": "^3.0.4",
    "body-parser": "^1.19.0",
    "chromatism2": "^3.0.2",
    "color": "^4.0.2",
    "cs-gamestate": "^2.2.3",
    "electron-updater": "^4.6.1",
    "express": "^4.17.1",
    "ffi-napi": "^4.0.3",
    "font-list": "^1.4.5",
    "http-proxy": "^1.18.1",
    "lodash": "^4.17.21",
    "maska": "^1.5.0",
    "mixpanel": "^0.16.0",
    "ms-speech-api": "^0.0.1",
    "nanoid": "^4.0.0",
    "obs-websocket-js": "^5.0.1",
    "pinia": "^2.0.29",
    "rcon": "^1.1.0",
    "recursive-readdir": "^2.2.2",
    "ref-union-di": "^1.0.1",
    "regedit": "^5.0.0",
    "say": "^0.16.0",
    "thumbsupply": "^0.4.0",
    "win32-api": "^19.8.1",
    "winston": "^3.3.3",
    "ws": "^8.2.3",
    "yaml": "^2.1.1"
  },
  "devDependencies": {
    "@jaames/iro": "^5.5.2",
    "@mdi/font": "^6.5.95",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@vitejs/plugin-vue": "^3.0.3",
    "@volar-plugins/prettyhtml": "^1.0.0",
    "electron": "20.0.1",
    "electron-builder": "^23.3.3",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^8.17.0",
    "vite-plugin-electron": "^0.9.0",
    "vite-plugin-electron-renderer": "^0.9.0",
    "vite-plugin-vuetify": "^1.0.0-alpha.12",
    "vue": "^3.2.37",
    "vue-mixpanel": "^2.0.0",
    "vue-router": "^4.0.16",
    "vuedraggable": "^4.1.0",
    "vuetify": "^3.0.0-beta.10",
    "vuex": "^4.0.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "strict": "off",
      "no-unused-vars": "off",
      "vue/no-unused-components": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}

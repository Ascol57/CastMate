{
	"compilerOptions": {
		"target": "ES2022",
		"module": "ESNext",
		"lib": ["ES2022", "DOM", "DOM.Iterable"],
		"sourceMap": true,
		"noImplicitAny": true,
		"removeComments": true,
		"preserveConstEnums": true,
		"strictNullChecks": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"moduleResolution": "bundler",
		"types": ["vite/client"]
	}
}
